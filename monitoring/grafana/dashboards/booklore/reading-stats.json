        "valueFormat": "time"
      },
      "pluginVersion": "1.0.4",
      "targets": [
        {
          "datasource": { "type": "mysql", "uid": "booklore-mariadb" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  DATE(start_time) as time,\n  ROUND(SUM(duration_seconds)/60, 0) as value\nFROM reading_sessions\nWHERE user_id = 1\n  AND YEAR(start_time) = YEAR(CURDATE())\nGROUP BY DATE(start_time)\nORDER BY time",
          "refId": "A"
        }
      ],
      "title": "Reading Activity (This Year) - Minutes per Day",
      "type": "tim012432-calendarheatmap-panel"
    },
