        {
          "datasource": { "type": "mysql", "uid": "booklore-mariadb" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  DAYNAME(start_time) as weekday,\n  COUNT(*) as sessions,\n  ROUND(SUM(duration_seconds)/3600, 1) as duration_hours\nFROM reading_sessions\nWHERE user_id = 1\nGROUP BY DAYOFWEEK(start_time), DAYNAME(start_time)\nORDER BY DAYOFWEEK(start_time)",
          "refId": "A"
        }
      ],
