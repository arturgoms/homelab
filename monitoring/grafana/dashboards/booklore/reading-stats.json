        {
          "datasource": { "type": "mysql", "uid": "booklore-mariadb" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  CAST(r.rating AS CHAR) as rating,\n  COALESCE(COUNT(ubp.id), 0) as books\nFROM (\n  SELECT 1 as rating UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5\n  UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10\n) r\nLEFT JOIN user_book_progress ubp ON ubp.personal_rating = r.rating AND ubp.user_id = 1\nGROUP BY r.rating\nORDER BY r.rating",
          "refId": "A"
        }
      ],
      "title": "Personal Rating Distribution",
      "type": "barchart"
